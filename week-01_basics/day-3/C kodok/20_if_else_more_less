#include <stdio.h>
#include <stdlib.h>

/*
swap the values of the to number
print out first the original values then print out the swaped values
but this time use bitwise operators only
*/

int main()
{
    int x = 32;
    int y = 20;
    int z;
    z = x;
    x = y;
    y = z;

    printf( " x : %d, y : %d \n", x, y)  ;

    return 0;

}
